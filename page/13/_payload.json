,{"title":103,"searchDepth":3,"depth":3,"links":492},[493,494],{"id":223,"depth":3,"text":223},{"id":290,"depth":3,"text":290},{"title":496,"date":215,"path":497,"tags":498,"body":500},"git笔记","/2020/07/11/gitnotes",[10,499],"笔记",{"type":14,"value":501,"toc":671},[502,506,509,523,526,529,538,541,544,553,556,559,568,571,574,577,646,649,658,661,667],[503,504,505],"h3",{"id":505},"git自动填入账号密码",[17,507,508],{},"打开终端，输入",[135,510,514],{"className":511,"code":512,"language":513,"meta":103,"style":103},"language-shell shiki shiki-themes one-light one-dark-pro","git config --global credential.helper store\n","shell",[142,515,516],{"__ignoreMap":103},[517,518,521],"span",{"class":519,"line":520},"line",1,[517,522,512],{},[17,524,525],{},"此时，我们就已经开启了git账号密码的本地储存，在下一次push时只要输入账号密码就可以一劳永逸了。",[503,527,528],{"id":528},"git设置默认的commit编辑器",[135,530,532],{"className":511,"code":531,"language":513,"meta":103,"style":103},"git config --global core.editor $editor_name\n",[142,533,534],{"__ignoreMap":103},[517,535,536],{"class":519,"line":520},[517,537,531],{},[17,539,540],{},"Ps: $editor_name指的是你选用的编辑器，一般为nano、vim等",[503,542,543],{"id":543},"pick一个仓库中连续的几个commit",[135,545,547],{"className":511,"code":546,"language":513,"meta":103,"style":103},"git cherry-pick \u003Ccommit1_id>..^\u003Ccimmitn_id>\n",[142,548,549],{"__ignoreMap":103},[517,550,551],{"class":519,"line":520},[517,552,546],{},[17,554,555],{},"Ps: \u003Ccommit1_id>和\u003Ccommitn_id>分别指第一个你想要pick的commit_id和最后一个你想要pick的commit_id",[503,557,558],{"id":558},"pick失败时如何撤销此次pick",[135,560,562],{"className":511,"code":561,"language":513,"meta":103,"style":103},"git cherry-pick --abort\n",[142,563,564],{"__ignoreMap":103},[517,565,566],{"class":519,"line":520},[517,567,561],{},[117,569,570],{"id":570},"踩坑记录",[17,572,573],{},"发生背景：",[17,575,576],{},"clone了一个内核仓库，大概是1.4G左右的大小，在github新建了一个repository，打算push上去，报错如下",[135,578,580],{"className":511,"code":579,"language":513,"meta":103,"style":103},"[zhullyb@Archlinux sdm845]$ git push -u ori